{
    "include": [
        "services",
        "data",
        "tools"
    ],
    "exclude": [
        "venv",
        "**/__pycache__",
        "**/*.pyc"
    ],
    "extraPaths": [
        "."
    ],
    "pythonVersion": "3.11",
    "pythonPlatform": "Windows",
    "typeCheckingMode": "basic",
    "reportMissingImports": "none",
    "reportMissingTypeStubs": false,
    "reportImportCycles": "warning",
    "reportUnusedImport": "information",
    "reportUnusedVariable": "information"
}
